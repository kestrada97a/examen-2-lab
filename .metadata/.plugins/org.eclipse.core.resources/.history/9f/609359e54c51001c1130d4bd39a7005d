//brief Crea memoria para un nuevo elemento editorial
//param void
//param void
//return pEditorial
eEditorial* editorial_new()
{
	eEditorial* pEditorial;
	pEditorial= (eEditorial*)malloc(sizeof(eEditorial));

	return pEditorial;
}

